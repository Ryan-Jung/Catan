{
  "name": "catan",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "NODE_ENV=development DEBUG=catan:* nodemon ./bin/www",
    "db:create:migration": "sequelize migration:generate --name ",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "postinstall": "sequelize db:migrate"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "express": "~4.15.5",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "pg-promise": "^8.2.2",
    "pug": "^2.0.3",
    "sequelize": "^4.37.4",
    "sequelize-cli": "^4.0.0",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "nodemon": "^1.17.2"
  }
}
